  "homepage": "./",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn prettier",
      "cross-env NODE_ENV=development eslint --fix",
      "git add"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc,stylelintrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint --ignore-path .eslintignore '**/*.*(css|scss)' --syntax scss",
      "git add"
    ],
    "*.{yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write",
      "git add"
    ]
  },
  "main": "./app/main.prod.js",
  "private": true,
  "productName": "Nebula Omega",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nebulabots/omega.git"
  },
  "engines": {
    "node": ">14.0"
  },
  "scripts": {
    "b": "babel-node --extensions \".es6,.js,.es,.jsx,.mjs,.ts,.tsx\"",
    "build": "yarn lint && concurrently \"yarn build-main\" \"yarn build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --config ./webpack/config.renderer.dev.dll.babel.js --colors",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --config ./webpack/config.main.prod.babel.js --colors",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --config ./webpack/config.renderer.prod.babel.js --colors",
    "clean": "cross-env rm -rf node_modules/ && rm -rf dist/ && rm -rf dll/ && rm -rf yarn-error.log && rm -rf .eslintcache && rm -rf app/dist && rm -rf app/main.prod.js",
    "dev": "cross-env START_HOT=1 node -r @babel/register ./internals/scripts/CheckPortInUse.js && cross-env START_HOT=1 yarn start-renderer-dev",
    "lint": "cross-env NODE_ENV=development eslint --cache --format=pretty --ext .js --ext .jsx --ext .ts --ext .tsx .",
    "lint-fix": "yarn --silent lint --fix",
    "lint-styles": "stylelint --ignore-path .eslintignore '**/*.*(css|scss)' --syntax scss",
    "lint-styles-fix": "yarn --silent lint-styles --fix",
    "package-all": "yarn build && electron-builder -mw --publish never",
    "package-linux": "yarn build && build --linux --publish never",
    "package-mac": "yarn build && electron-builder --mac --publish never",
    "package-win": "yarn build && electron-builder --win --x64 --publish never",
    "postinstall": "cross-env yarn build-dll && electron-builder install-app-deps",
    "postlint-fix": "prettier --ignore-path .eslintignore --single-quote --write '**/*.*{js,jsx,ts,tsx,json}'",
    "postlint-styles-fix": "prettier --ignore-path .eslintignore --single-quote --write '**/*.{css,scss}'",
    "preinstall": "node ./internals/scripts/CheckYarn.js",
    "prestart": "yarn build",
    "prettier": "prettier --write --single-quote true --trailing-comma all --print-width 120",
    "publish-all": "yarn build && electron-builder -mw --publish always",
    "publish-linux": "yarn build && electron-builder --linux --publish always",
    "publish-mac": "yarn build && electron-builder --mac --publish always",
    "publish-win": "yarn build && electron-builder --win --x64 --publish always",
    "start": "cross-env DEBUG_PROD=true NODE_ENV=production run-electron ./app/main.prod.js",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development run-electron -r ./test/babelRegisterTs.js app/main.dev.ts",
    "start-renderer-dev": "cross-env NODE_ENV=development UV_THREADPOOL_SIZE=1024 webpack-dev-server --config webpack/config.renderer.dev.babel.js",
    "typecheck": "tsc --pretty"
  }
}
